{
  "name": "nextxp-demo",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "npm run server-build && cross-env NODE_ENV=development node dist/server/server.js",
    "start": "cross-env NODE_ENV=production node dist/server/server.js",
    "prod": "npm run build && npm run start",
    "build": "npm run client-build && npm run server-build",
    "client-build": "npx next build",
    "server-build": "tsc --build ./server/tsconfig.json",
    "lint": "npx next lint"
  },
  "dependencies": {
    "@enonic/nextjs-adapter": "^1.0.2",
    "cross-env": "^7.0.3",
    "html-react-parser": "^3.0.15",
    "next": "^13.3.1",
    "next-transpile-modules": "^10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "unescape": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@types/react": "18.0.38",
    "@types/react-dom": "^18.0.11",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "prettier": "^2.8.8",
    "typescript": "5.0.4"
  }
}
